@model IEnumerable<StoreData.Models.Products>
@foreach (var item in Model)
{
<div class="col-6 col-md-3  text-center mt-5  mb-3">
    <a class="mt-2" href="#" style="font-size: 20px;color: black;text-decoration:none;" target="_blank">
        <img class="pho mt-3" src='https://placehold.it/150x150' alt='' />
    </a>
    @Html.ActionLink("")
    <a href="#">@item.ProductName</a>
    <br>
    <a>$@item.UnitPrice</a>
</div>
}
